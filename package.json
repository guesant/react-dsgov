{
  "name": "@guesant/react-dsgov",
  "description": "Componentes React do DSGov (PadrÃ£o Digital do Governo).",
  "author": "cosis-stn",
  "version": "0.7.2-beta.1",
  "private": false,
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosis-stn/react-dsgov.git"
  },
  "homepage": "https://cosis-stn.github.io/react-dsgov/styleguide/",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "ui",
    "dsgov"
  ],
  "main": "dist/cjs/index.min.js",
  "module": "dist/esm/index.min.js",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@govbr-ds/core": "^3.0.1",
    "classnames": "^2.3.1",
    "font-awesome": "^4.7.0",
    "lodash.uniqueid": "^4.0.1",
    "react-any-attr": "^1.1.2",
    "react-fontawesome": "^1.7.1",
    "react-lorem-ipsum": "^1.4.10",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:bundles": "rollup -c",
    "build": "npm run buildbabel && npm run build:bundles",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build:m:base": "babel --extensions '.js,.jsx,.ts,.tsx' src/components",
    "build:m:esm": "TARGET_MODULE_SYSTEM=esm npm run build:m:base -- --out-dir dist/esm",
    "build:m:cjs": "TARGET_MODULE_SYSTEM=commonjs npm run build:m:base -- --out-dir dist/cjs",
    "build:m": "npm run build:m:cjs && npm run build:m:esm && rollup -c rollup.config.baseStyles.js",
    "build:types:base": "tsc -p . --outDir dist --declaration --emitDeclarationOnly --declarationDir",
    "build:types:esm": "npm run build:types:base dist/esm",
    "build:types:cjs": "npm run build:types:base dist/cjs",
    "build:types": "npm run build:types:cjs && npm run build:types:esm",
    "buildbabel": "rm -rf dist && NODE_ENV=production npm run build:m && npm run build:types"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-plugin-react-css-modules": "^5.2.6",
    "react-scripts": "^5.0.1",
    "sass": "^1.54.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/lodash.uniqueid": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "react-docgen-typescript": "^2.2.2",
    "react-styleguidist": "^11.2.0",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.73.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
